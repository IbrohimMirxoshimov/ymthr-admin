{
  "name": "ymthr-app",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate ./src/database/migrations/NewMigration",
    "migration:create": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "seed": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "reseed": "npm run schema:drop && npm run seed",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "postinstall": "is-ci || husky install",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:query": "nodemon -r tsconfig-paths/register ./src/database/seeds/test-query.ts",
    "test:plain": "nodemon test/plain.ts",
    "seed:extra": "ts-node -r tsconfig-paths/register ./src/database/seeds/extra-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "6.0.5",
    "@nestjs/typeorm": "9.0.1",
    "@types/source-map-support": "^0.5.6",
    "axios": "^1.4.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "google-auth-library": "8.4.0",
    "handlebars": "4.7.7",
    "lodash": "^4.17.21",
    "multer": "1.4.4",
    "multer-s3": "2.10.0",
    "nestjs-i18n": "9.1.6",
    "node-fetch": "^3.3.0",
    "nodemailer": "6.7.8",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "4.5.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.11",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.186",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.56",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "env-cmd": "10.1.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}